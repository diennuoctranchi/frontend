<script type="application/ld+json">
{
  "@context": "https://schema.org/",
  "@graph":
  [
    {
      "@type": "BreadcrumbList",
      "itemListElement": [{
        "@type": "ListItem", 
        "position": 1, 
        "name": "Trang Chủ",
        "item": "https://diennuoctranchi.com/"  
      },{
        "@type": "ListItem", 
        "position": 2, 
        "name": "<%= @service_category.get_name %>",
        "item": "https://diennuoctranchi.com<%= service_list_link(@service_category) %>"  
      }]
    },
    {
      "@type": "Article",
      "mainEntityOfPage": {
        "@type": "WebPage",
        "@id": "<%= service_list_link(@service_category) %>"
      },
      "headline": "<%= @page_title.html_safe %>",
      "description": "<%= @meta_description.html_safe %>",
      "image": "https://diennuoctranchi.com/img/banner-share.jpg",  
      "author": {
        "@type": "Organization",
        "name": "Điện Nước Trần Chí",
        "url": "https://diennuoctranchi.com/"
      },  
      "publisher": {
        "@type": "Organization",
        "name": "Điện Nước Trần Chí",
        "logo": {
          "@type": "ImageObject",
          "url": "https://diennuoctranchi.com/img/logo.png"
        }
      },
      "datePublished": "<%= @service_category.created_at.strftime("%d/%m/%Y") %>",
      "dateModified": "<%= @service_category.updated_at.strftime("%d/%m/%Y") %>"
    }
  ]
}
</script>
<meta property="og:title" content="<%= @page_title.html_safe %>">
<meta property="og:site_name" content="Điện Nước Trần Chí">
<meta property="og:url" content="https://diennuoctranchi.com<%= service_list_link(@service_category) %>">
<meta property="og:description" content="<%= @meta_description.html_safe %>">
<meta property="og:type" content="website">
<meta property="og:image" content="https://diennuoctranchi.com/img/banner-share.jpg">

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@diennuoctranchi">
<meta name="twitter:title" content="<%= @page_title.html_safe %>">
<meta name="twitter:description" content="<%= @meta_description.html_safe %>">
<meta name="twitter:image" content="https://diennuoctranchi.com/img/banner-share.jpg">