<script type="application/ld+json">
{
  "@context": "https://schema.org/",
  "@graph":
  [
    {
      "@type": "BreadcrumbList",
      "itemListElement": [{
        "@type": "ListItem", 
        "position": 1, 
        "name": "Trang Chủ",
        "item": "https://diennuoctranchi.com/"  
      },{
        "@type": "ListItem", 
        "position": 2, 
        "name": "Câu Hỏi Thường Gặp",
        "item": "https://diennuoctranchi.com/cau-hoi-thuong-gap"  
      }]
    },
    {
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity":
      [
        <% faq_count = @faqs.count %>
        <% @faqs.each_with_index do |faq, index| %>
          <% if (index+1) == (faq_count) %>
            {
              "@type": "Question",
              "name": "<%= faq.get_question %>",
              "acceptedAnswer": {
                "@type": "Answer",
                "text": "<%= faq.get_reply %>"
              }
            }
          <% else %>
            {
              "@type": "Question",
              "name": "<%= faq.get_question %>",
              "acceptedAnswer": {
                "@type": "Answer",
                "text": "<%= faq.get_reply %>"
              }
            },
          <% end %>
        <% end %>
      ]
    }
  ]
}
</script>
<meta property="og:title" content="<%= @page_title.html_safe %>">
<meta property="og:site_name" content="Điện Nước Trần Chí">
<meta property="og:url" content="https://diennuoctranchi.com/cau-hoi-thuong-gap">
<meta property="og:description" content="<%= @meta_description.html_safe %>">
<meta property="og:type" content="website">
<meta property="og:image" content="https://diennuoctranchi.com/img/banner-share.jpg">

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@diennuoctranchi">
<meta name="twitter:title" content="<%= @page_title.html_safe %>">
<meta name="twitter:description" content="<%= @meta_description.html_safe %>">
<meta name="twitter:image" content="https://diennuoctranchi.com/img/banner-share.jpg">